<Window x:Class="Instagram.MainWindow"
        Loaded="Window_Loaded"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Instagram"
        mc:Ignorable="d"
        Title="MainWindow" Height="538" Width="721">
    <Grid Margin="0,0,0,-6">
        <Grid HorizontalAlignment="Left" Width="721">
            <Grid.RowDefinitions>
                <RowDefinition Height="*"/>
                <RowDefinition Height="Auto"/>
            </Grid.RowDefinitions>
            <ScrollViewer Grid.Row="0" HorizontalAlignment="Left" Width="583" Margin="10,0,0,0">
                <StackPanel x:Name="panel" Orientation="Vertical" Height="Auto" Width="423"/>
            </ScrollViewer>
            <Button x:Name="BtnUpload" Content="Upload" Margin="598,40,24,432" Click="BtnUpload_Click" FontFamily="Arial Rounded MT Bold" BorderBrush="Black" Background="White" Foreground="#FF0004FF" FontSize="22" />
        </Grid>
    </Grid>
</Window>
